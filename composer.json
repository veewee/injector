{
    "name": "veewee/injector",
    "description": "Copy / paste code at specific tokens in a list of PHP files",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Injector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InjectorTest\\": "test/"
        }
    },
    "require": {
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "sebastian/diff": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "php-vfs/php-vfs": "^1.4",
        "phpstan/phpstan": "^0.9.2",
        "phpro/grumphp": "^0.14.0"
    }
}
